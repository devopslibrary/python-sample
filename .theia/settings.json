{
    "workbench.iconTheme": "theia-file-icons",
    "python.autoComplete.extraPaths": [
        ".venv/lib/python3.8/site-packages",
        ".venv/lib64/python3.8/site-packages"
    ],
    "editor.autoSave": "on",
    "editor.minimap.enabled": true,
    "editor.rulers": [
        88
    ],
    "python.pythonPath": ".venv/bin/python3",
    "files.exclude": {
        ".git": true,
        ".pytest_cache": true,
        ".venv": true,
        "**/__pycache__": true,
        ".mypy_cache": true,
        "*.egg-info": true
    }
}